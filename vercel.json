{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/send-email",
        "dest": "/send-email"
      },
      {
        "src": "/status",
        "dest": "/status"
      }
    ]
   
  }
  